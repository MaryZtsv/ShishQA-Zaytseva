		{
			"name": "New Issue",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var rmd = Math.random();\r",
							"pm.collectionVariables.set(\"summary\",\"Zaytseva\" + rmd);\r",
							"pm.collectionVariables.set(\"description\",\"Это поле описания\" + rmd );"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"setIssueIdVariable\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.collectionVariables.set(\"issueId\", jsonData.id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"project\": {\r\n        \"id\": \"{{ProjectId}}\"\r\n    },\r\n    \"summary\": \"{{summary}}\",\r\n    \"description\":\"{{description}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{URL}}/api/issues",
					"host": [
						"{{URL}}"
					],
